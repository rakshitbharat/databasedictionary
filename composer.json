{
    "name": "rakshitbharat/databasedictionary",
    "description": "Make a Database Dictionary with Entity (Annotation) and maintain database schema with ease.",
    "keywords": [
        "rakshitbharat",
        "databasedictionary"
    ],
    "homepage": "https://github.com/rakshitbharat/databasedictionary",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rakshit Patel",
            "email": "rakshitbharatproject@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Rakshitbharat\\DatabaseDictionary\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rakshitbharat\\DatabaseDictionary\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rakshitbharat\\DatabaseDictionary\\DatabaseDictionaryServiceProvider"
            ],
            "aliases": {
                "DatabaseDictionary": "Rakshitbharat\\DatabaseDictionary\\DatabaseDictionaryFacade"
            }
        }
    }
}
